#!/usr/bin/env node

var Server = require('../').Server;
var server = new Server({
  flushInterval: 1000
});

server.start()

  .then(function () {

    server.on('flush', function (timestamp, metrics) {

      console.log(timestamp);
      console.log(metrics);
      console.log();

    });

  })

  .catch(console.error);
